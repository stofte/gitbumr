{
    "cppFile": "app/Bindings.cpp",
    "rust": {
        "dir": "lib",
        "interfaceModule": "interface",
        "implementationModule": "implementation"
    },
    "objects": {
        "Repositories": {
            "type": "List",
            "itemProperties": {
                "id": {
                    "type": "qint64",
                    "write": false
                },
                "displayName": {
                    "type": "QString",
                    "write": false
                },
                "current": {
                    "type": "bool",
                    "write": false
                }
            },
            "properties": {
                "activeRepository": { "type": "QString" }
            },
            "functions": {
                "init": {
                    "return": "void",
                    "mut": true,
                    "arguments": [
                        { "name": "db_file_name", "type": "QString" }
                    ]
                },
                "add": {
                    "return": "bool",
                    "mut": true,
                    "arguments": [
                        { "name": "path", "type": "QString" }
                    ]
                },
                "addLastError": {
                    "return": "QString"
                },
                "remove": {
                    "return": "bool",
                    "mut": true,
                    "arguments": [
                        { "name": "index", "type": "quint64" }
                    ]
                },
                "setCurrent": {
                    "return": "void",
                    "mut": true,
                    "arguments": [
                        { "name": "id", "type": "qint64" }
                    ]
                }
            }
        },
        "Git": {
            "type": "Object",
            "properties": {
                "branches": { "type": "Branches" },
                "commit": { "type": "Commit" },
                "diffs": { "type": "Diffs" },
                "hunks": { "type": "Hunks" },
                "revwalkFilter": { "type": "QString" }
            },
            "functions": {
                "load": {
                    "return": "void",
                    "mut": true,
                    "arguments": [{ "name": "path", "type": "QString" }]
                },
                "loadCommit": {
                    "return": "void",
                    "mut": true,
                    "arguments": [
                        { "name": "oid", "type": "QString" }
                    ]
                },
                "loadDiff": {
                    "return": "void",
                    "mut": true,
                    "arguments": [
                        { "name": "oid", "type": "QString" },
                        { "name": "index", "type": "quint64" }
                    ]
                }
            }
        },
        "Commit": {
            "type": "Object",
            "properties": {
                "cid": { "type": "QString" },
                "time": { "type": "QString" },
                "author": { "type": "QString" },
                "committer": { "type": "QString" },
                "message": { "type": "QString" },
                "tree": { "type": "QString" }
            }
        },
        "Diffs": {
            "type": "List",
            "itemProperties": {
                "filenameOld": { "type": "QString" },
                "filenameNew": { "type": "QString" },
                "status": { "type": "QString" },
                "patch": { "type": "QString" }
            },
            "properties": {
                "commitOid": { "type": "QString" },
                "maxFilenameLength": { "type": "quint64" }
            }
        },
        "Hunks": {
            "type": "List",
            "itemProperties": {
                "hunk": { "type": "QString" },
                "hunkMaxLineLength": { "type": "quint64" },
                "linesNewFrom": { "type": "quint64" },
                "linesNewTo": { "type": "quint64" },
                "linesOrigin": { "type": "QByteArray" },
                "linesNew": { "type": "QByteArray" },
                "linesNewCols": { "type": "quint64" },
                "linesOld": { "type": "QByteArray" },
                "linesOldCols": { "type": "quint64" }
            }
        },
        "Branches": {
            "type": "List",
            "itemProperties": {
                "name": { "type": "QString" },
                "checkedout": { "type": "bool" },
                "oid": { "type": "QString" }
            }
        },
        "Log": {
            "type": "List",
            "itemProperties": {
                "cid": { "type": "QString" },
                "timeHumanized": { "type": "QString" },
                "time": { "type": "QString" },
                "author": { "type": "QString" },
                "message": { "type": "QString" },
                "summary": { "type": "QString" },
                "graph": { "type": "QByteArray" },
                "isMerge": { "type": "bool" }
            },
            "functions": {
                "load": {
                    "return": "void",
                    "mut": true,
                    "arguments": [{ "name": "path", "type": "QString" }]
                },
                "filter": {
                    "return": "void",
                    "mut": true,
                    "arguments": [{ "name": "filter", "type": "QString" }]
                }
            }
        }
    }
}
