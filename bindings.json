{
    "cppFile": "app/Bindings.cpp",
    "rust": {
        "dir": "lib",
        "interfaceModule": "interface",
        "implementationModule": "implementation"
    },
    "objects": {
        "App": {
            "type": "Object",
            "properties": {
                "repositories": { "type": "Repositories" },
                "activeRepository": { "type": "quint64" },
                "activeRepositoryPath": { "type": "QString" },
                "activeRepositoryDisplayName": { "type": "QString" }  
            },
            "functions": {
                "init": {
                    "return": "void",
                    "mut": true
                },
                "addRepository": {
                    "return": "quint64",
                    "mut": true,
                    "arguments": [
                        {"name": "path", "type": "QString"}
                    ]
                },
                "addRepositoryGetLastError": {
                    "return": "QString"
                },
                "repositoryIndex": {
                    "return": "quint64",
                    "mut": false,
                    "arguments": [
                        {"name": "id", "type": "quint64"}
                    ]
                },
                "setActiveRepository": {
                    "return": "void",
                    "mut": true,
                    "arguments": [
                        { "name": "id", "type": "quint64" }
                    ]
                }
            }
        },
        "Repositories": {
            "type": "List",
            "itemProperties": {
                "id": {
                    "type": "quint64",
                    "write": false
                },
                "displayName": {
                    "type": "QString",
                    "write": false
                },
                "current": {
                    "type": "bool",
                    "write": true
                }
            },
            "functions": {
                "add": {
                    "return": "bool",
                    "mut": true,
                    "arguments": [
                        { "name": "index", "type": "quint64" },
                        { "name": "path", "type": "QString" }
                    ]
                },
                "remove": {
                    "return": "bool",
                    "mut": true,
                    "arguments": [{
                        "name": "index",
                        "type": "quint64"
                    }]
                }
            }
        },
        "History": {
            "type": "List",
            "itemProperties": {
                "oid": { "type": "QString" },
                "time": { "type": "QString" },
                "author": { "type": "QString" },
                "message": { "type": "QString" }
            },
            "functions": {
                "load": {
                    "return": "void",
                    "mut": true,
                    "arguments": [{ "name": "path", "type": "QString" }]
                }
            }
        }
    }
}
